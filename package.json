{
  "name": "strever-challenge",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "prepare": "husky",
    "seed": "prisma db seed",
    "test": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:ci": "jest --watchAll=false && npm run test:e2e"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "overrides": {
    "swagger-ui-react": {
      "react": "$react",
      "react-dom": "$react-dom",
      "prismjs": "^1.30.0"
    }
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.28.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@edge-runtime/jest-environment": "^4.0.0",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "undici": "^7.15.0"
  }
}
